#!name= Telegram 客户端跳转
#!desc= 将 t.me 链接跳转到指定的 Telegram 第三方客户端（Turrit/SwiftGram）。通过配置界面选择客户端并控制启用/禁用。
#!author= lucasss1916
#!select = client_type,Turrit,SwiftGram
#!select = enabled,true,false
#!tag = Telegram第三方客户端跳转

[Argument] 
custom_client = select,Turrit,SwiftGram,tag=选择客户端,desc=选择目标客户端

[Rewrite]
# 拦截所有 t.me 的 HTTP/HTTPS 请求
^https?://t\.me/.*$ type=redirect,to={custom_client}://t.me/$1

[mitm]
hostname = t.me
